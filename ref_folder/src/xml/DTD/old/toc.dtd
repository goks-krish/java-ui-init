<!-- TOC elements and attributes. -->
<!ELEMENT	book	(booktitle | isbn | authors | cover | topic)*>
<!ELEMENT	cover	(#PCDATA)*>
<!ELEMENT	booktitle	(#PCDATA)*>
<!ELEMENT	title	(#PCDATA | xr | nb | sb | sp | em | img)*>
<!ELEMENT	authors	(#PCDATA)*>
<!ELEMENT	author	(#PCDATA)*>
<!ELEMENT	isbn	(#PCDATA)*>
<!ELEMENT	topic	(title | author|topic)*>
<!ATTLIST	topic	
	id	CDATA	#IMPLIED
	head-level	CDATA #IMPLIED
	lid	CDATA #IMPLIED>
<!ELEMENT sb	(#PCDATA)	>
<!ELEMENT	img	EMPTY>
<!ATTLIST	img
	src	CDATA	#IMPLIED
	border	CDATA	#IMPLIED>
<!ELEMENT sp	(#PCDATA)	>	
<!ELEMENT em	(#PCDATA)	>
<!ATTLIST	em
	activity	CDATA	#IMPLIED>
<!ELEMENT xr	(#PCDATA)	>
<!ATTLIST	xr
	linkends	CDATA	#IMPLIED>
<!ELEMENT nb	(#PCDATA)	>
<!ATTLIST	nb
	linkend	CDATA	#IMPLIED>
